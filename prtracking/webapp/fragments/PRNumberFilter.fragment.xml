<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Purchase Req. Number"
		items="{
            path: 'purchReqNumberModel>/',
            sorter: { path: 'EX_BANFN' }                                        
        }" 
		search="searchPRNumber"
		confirm="closeValueHelpPRNumber"
		cancel="closeValueHelpPRNumber">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{purchReqNumberModel>EX_TXZ01}"
			description="{purchReqNumberModel>EX_BANFN}" />
	</SelectDialog>
</core:FragmentDefinition>